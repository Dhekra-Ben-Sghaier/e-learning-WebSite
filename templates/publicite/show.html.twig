{% extends 'base_admin.html.twig' %}

{% block title %}Publicite{% endblock %}

{% block content %}
    <h1>Publicite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ publicite.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ publicite.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ publicite.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ publicite.email }}</td>
            </tr>
            <tr>
                <th>Domaine</th>
                <td>{{ publicite.domaine }}</td>
            </tr>
            <tr>
                <th>Affichage</th>
                <td>{{ publicite.affichage }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ publicite.image }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ publicite.prix }}</td>
            </tr>
            <tr>
                <th>Lien</th>
                <td>{{ publicite.lien }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('publicite_index') }}">back to list</a>

    <a href="{{ path('publicite_edit', {'id': publicite.id}) }}">edit</a>

    {{ include('publicite/_delete_form.html.twig') }}
{% endblock %}
